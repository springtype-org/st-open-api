{
  "name": "st-open-api",
  "version": "1.4.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "open-api",
    "swagger",
    "client-generator",
    "springtype"
  ],
  "description": "st-open-api generates API client SDKs from an OpenAPI specification written in OpenAPI version 3.x.x",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/springtype-org/st-open-api.git"
  },
  "scripts": {
    "start": "node dist/cli.js",
    "watch": "tsc --watch",
    "prebuild": "npx st-rm-rf dist",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "node postbuild.js && cd dist",
    "test": "jest"
  },
  "engines": {
    "node": ">=16"
  },
  "bundledDependencies": [
    "ajv",
    "commander",
    "mustache",
    "node-fetch",
    "st-rm-rf",
    "valid-url",
    "typescript"
  ],
  "bin": "./cli.js",
  "author": "Michael Mannseicher, Aron Homberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springtype-org/st-open-api/issues"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "commander": "^5.1.0",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "mustache": "^4.2.0",
    "node-fetch": "^2.6.1",
    "st-rm-rf": "^2.0.0-beta.1",
    "typescript": "^4.7.4",
    "valid-url": "^1.0.9",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/mustache": "^4.0.1",
    "@types/node": "14.14.2",
    "@types/node-fetch": "^2.5.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^0.25.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "json-schema-faker": "^0.5.0-rcv.41",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
  }
}
